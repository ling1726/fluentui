{
  "name": "vr-tests",
  "version": "1.0.0",
  "private": true,
  "description": "Visual regression tests for Fluent UI React",
  "scripts": {
    "build": "just-scripts build",
    "clean": "just-scripts clean",
    "code-style": "just-scripts code-style",
    "just": "just-scripts",
    "lint": "just-scripts lint",
    "screener": "screener-storybook --conf screener.config.js --debug",
    "screener:build": "cross-env NODE_OPTIONS=--max-old-space-size=3072 just-scripts storybook:build",
    "screener:local": "screener-storybook --conf screener.local.config.js --debug",
    "start": "just-scripts dev:storybook"
  },
  "devDependencies": {
    "@fluentui/eslint-plugin": "*"
  },
  "dependencies": {
    "@fluentui/example-data": "^8.3.0",
    "@fluentui/font-icons-mdl2": "^8.1.19",
    "@fluentui/react": "^8.48.0",
    "@fluentui/react-experiments": "^8.4.10",
    "@fluentui/react-hooks": "^8.3.8",
    "@fluentui/react-icons": "^2.0.154-beta.5",
    "@fluentui/react-icons-mdl2": "^1.2.10",
    "@fluentui/scripts": "^1.0.0",
    "@fluentui/storybook": "^1.0.0",
    "@types/react": "16.9.42",
    "@types/react-dom": "16.9.10",
    "csstype": "^2.6.7",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "screener-storybook": "0.21.2",
    "tslib": "^2.1.0"
  }
}
